{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"globals": {
		"L": "readonly",
		"Toastify": "readonly"
	},
	"extends": [
		"htmlacademy/vanilla",
		"plugin:import/recommended",
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"rules": {
		"no-param-reassign": 0,
		"no-console": "off"
	}
}
